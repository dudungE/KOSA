//*
package ex03.operator;

//최단산쉬관논삼대콤
public class OperatorTest {
	public static void main(String[] args) {
		// shift 연산자 :  <<, >>,  <<<
		
		int x = 8,  result;
		
//		result = x << 2;    // left shift :   원래값 * 2^bit수  ==>  8 * 4 = 32
		result = x >> 3;    // right shift : 
		
		
		System.out.println(" left shift result : " + result);
		
	}
}
//*/